# SRTM2TAK Environment Configuration
# Copy this file to .env.local for local development

# S3 Configuration
VITE_S3_BASE_URL=https://s3.amazonaws.com/elevation-tiles-prod/skadi

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_OFFLINE_MODE=true

# App Configuration
VITE_MAX_TILES_MOBILE=10
VITE_MAX_TILES_DESKTOP=50
VITE_MAX_CONCURRENT_DOWNLOADS=3

# Memory Limits (in MB)
VITE_MEMORY_WARNING_THRESHOLD=200
VITE_MEMORY_CRITICAL_THRESHOLD=400

# Cache Configuration
VITE_CACHE_TTL_DAYS=7
VITE_MAX_CACHE_SIZE_MB=500