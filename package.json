{
  "name": "srtm2tak",
  "version": "0.1.0",
  "description": "SRTM elevation data downloader and packager for ATAK",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/maplibre-gl": "^1.13.2",
    "@zip.js/zip.js": "^2.7.34",
    "idb": "^8.0.0",
    "maplibre-gl": "^5.7.1",
    "pako": "^2.1.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/node": "^24.3.3",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "fake-indexeddb": "^6.2.2",
    "jsdom": "^24.0.0",
    "jszip": "^3.10.1",
    "msw": "^2.1.4",
    "playwright": "^1.41.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.2.1"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
